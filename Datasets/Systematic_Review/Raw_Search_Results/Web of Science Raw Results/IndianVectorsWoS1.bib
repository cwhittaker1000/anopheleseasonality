@article{ ISI:000282994900004,
Author = {Mandal, Shyamapada},
Title = {{Exploration of larvicidal and adult emergence inhibition activities of
   Ricinus communis seed extract against three potential mosquito vectors
   in Kolkata, India}},
Journal = {{ASIAN PACIFIC JOURNAL OF TROPICAL MEDICINE}},
Year = {{2010}},
Volume = {{3}},
Number = {{8}},
Pages = {{605-609}},
Month = {{AUG}},
Abstract = {{Objective: To determine the larvicidal and adult emergence inhibition
   activities of castor (Ricinus communis) seed extract against three
   potential mosquito vectors Anopheles stephensi (An. stephensi), Culex
   quinquefasciatus (Cx. quinquefasciatus) and Aedes albopictus (Ae.
   albopictus) in India. Methods: The R. communis seed extract was tested,
   employing WHO procedure, against fourth larval instars of the three
   mosquito species for 24 h and larval Mortalities were recorded at
   various concentrations (2-64 mu g/mL); the 24 h LC50 values of the R.
   cowman's seed extract were determined following Probit analysis. The
   larval killing, antipupation and adult emergence inhibition rates of the
   test extract, using a single concentration of 2 mu LC50, were studied at
   different time periods (24-72 h); the extract toxicity was tested
   against a fish, Oreochromis niloticus (O. niloticus). Results: The R.
   communis seed extract exhibited larvicidal effects with 100 \% killing
   activities at concentrations 32-64 mu g/mL, and with LC50 values 7.10,
   11.64 and 16.84 mu g/mL for Cx. quinquefasciatus, An. stephensi and Ae.
   albopictus larvae, respectively. When the larvae were treated with the
   extract at a single concentration of 2 X LC50, significant differences
   were observed, compared to control groups, in rate of pupation (P<0.001)
   as well as in adult formation (P<0.001). Conclusions: The present
   findings suggest that the R. communis seed extract provided an excellent
   potential for controlling An. stephensi, Cx. quinquefasciatus and Ae.
   albopictus mosquito vectors.}},
