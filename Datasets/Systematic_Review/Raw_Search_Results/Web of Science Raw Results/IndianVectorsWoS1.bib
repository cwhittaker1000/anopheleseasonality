@article{ ISI:000236184600008,
Author = {Sunish, IP and Reuben, R and Rajendran, R},
Title = {{Natural survivorship of immature stages of Culex vishnui (Diptera :
   culicidae) complex, vectors of Japanese encephalitis virus, in rice
   fields in southern India}},
Journal = {{JOURNAL OF MEDICAL ENTOMOLOGY}},
Year = {{2006}},
Volume = {{43}},
Number = {{2}},
Pages = {{185-191}},
Month = {{MAR}},
Abstract = {{The development and survival of miniatures of Culex vishnui (Diptera:
   Culicidae) complex, vectors of Japanese encephalitis virus, were studied
   in transplanted rice, Oryza savita L., fields during three crop growing
   seasons. The total duration of development from the first instar to
   adult emergence varied from 6 to 8 d. Survival rate estimates ranged
   from 0.003 to 0.524, but they generally were < 0.1. Unusually high
   survival rates (0.192-0.524) were observed in summer and long-term
   monsoon crop seasons during 1993. A multiple regression method using
   backward elimination was used to analyze the factors responsible for
   these variations. The model identified nine parameters related with
   survival rates that explained 99\% of the variance. Area of water
   surface and hardness were negatively related, whereas green algae,
   notonectid adults, anisopterans, dytiscids, salinity, water depth, and
   dissolved oxygen were positively associated with immature survival.}},
