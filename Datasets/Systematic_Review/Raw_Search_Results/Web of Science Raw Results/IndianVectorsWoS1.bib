@article{ ISI:000188747100027,
Author = {Mohanty, SS and Prakash, S},
Title = {{Extracellular metabolites of Trichophyton ajelloi against Anopheles
   stephensi and Culex quinquefasciatus larvae}},
Journal = {{CURRENT SCIENCE}},
Year = {{2004}},
Volume = {{86}},
Number = {{2}},
Pages = {{323-325}},
Month = {{JAN 25}},
Abstract = {{Keratinophilic deuteromycetous fungus Trichophyton ajelloi was isolated
   from the soil by feather-baiting technique. It has previously been
   tested as a larvicidal agent of Anopheles stephensi and Culex
   quinquefasciatus. The extracellular metabolites of T. ajelloi were
   collected from the Sabouraud dextrose broth by filtering through
   Whatman-1 filter paper after 21 days of growth. The bioassays were run
   at six different concentrations chosen to produce larval mortalities
   between 20 and 95\% for calculating the LC50 and LC90 values by probit
   analysis. The larvae of Cx. quinquefasciatus were 0.25-fold more
   susceptible than the An. stephensi. The 1st instars of both the species
   were more susceptible than the 2nd and 3rd instars. These extracellular
   metabolites of T. ajelloi could be regarded as alternatives to synthetic
   insecticides. This indigenously developed larvicide needs to be explored
   further in different climatic zones of India.}},
Publisher = {{CURRENT SCIENCE ASSN}},
Address = {{C V RAMAN AVENUE, PO BOX 8005, BANGALORE 560 080, INDIA}},
Type = {{Article}},
