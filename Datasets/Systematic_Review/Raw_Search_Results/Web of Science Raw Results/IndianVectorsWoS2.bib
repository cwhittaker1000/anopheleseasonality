@article{ ISI:000087650000003,
Author = {Subbarao, SK and Kumar, KV and Nanda, N and Nagpal, BN and Dev, V and
   Sharma, VP},
Title = {{Cytotaxonomic evidence for the presence of Anopheles nivipes in India}},
Journal = {{JOURNAL OF THE AMERICAN MOSQUITO CONTROL ASSOCIATION}},
Year = {{2000}},
Volume = {{16}},
Number = {{2}},
Pages = {{71-74}},
Month = {{JUN}},
Abstract = {{Anopheles philipinensis mosquitoes were collected from 5 states in
   India: Assam, Meghalaya, Arunachal Pradesh, Manipur, and Nagaland.
   Half-gravid females were examined for variations in wing venation using
   the presector dark mark on vein I and polytene chromosomes derived from
   ovarian nurse cells. Polytene chromosomes were examined for diagnostic
   inversions, t on chromosome arm 2 and 1 on arm 5. Based on wing
   characteristics, both An. philippinensis and An. nivipes were
   identified. Polytene chromosome examinations revealed that all specimens
   from these 2 populations had 2t; 51 inversion genotype, a diagnostic
   character for An. nivipes. The wing character was not diagnostic;
   therefore, it was concluded that all the specimens examined were
   actually An. nivipes and not An. philippinensis. Further, the X
   chromosome was of x + (b) type, that is, the standard arrangement with
   reference to the inversion b, reported in the An. nivipes population in
   Thailand. This is the Ist report that unequivocally establishes the
   occurrence of An. nivipes in India and also shows that the adult wing
   character is not reliable in distinguishing An. philippinensis from An.
   nivipes, as has been observed in Thailand.}},
