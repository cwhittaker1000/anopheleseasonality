@article{ ISI:000088202200002,
Author = {Malhotra, PR and Jatav, PC and Chauhan, RS},
Title = {{Surface morphology of the egg of Anopheles stephensi stephensi sensu
   stricto (Diptera, Culicidae)}},
Journal = {{ITALIAN JOURNAL OF ZOOLOGY}},
Year = {{2000}},
Volume = {{67}},
Number = {{2}},
Pages = {{147-151}},
Abstract = {{Anopheles stephensi Listen, 1901, is a recognized vector of urban
   malaria in India, The species occurs in two forms, i.d., the type form
   A. stephensi stephensi, and the variety A. stephensi mysorensis. The
   surface morphology of the egg of A. stephensi stephensi sensu stricto is
   described with the aid of optical and scanning electron microscopy. The
   egg is shiny jet black, `skiff-shaped' in appearance with its anterior
   end slightly broader than the posterior one. The average length and
   width of the egg is 596 +/- 8.35 and 196 +/- 1.73 ((x) over bar +/- SE)
   mu m, respectively. The number of ridges on the membranous egg-float
   varies from 16 to 19. The entire surface of the egg, except the
   micropyle, is covered with outer chorionic cells which are mostly
   pentagonal or hexagonal. The tubercles are equispaced. The micropyle is
   located at the apex of the dorso-anterior end of the egg, and has a
   well-built collar with a minute aperture in the centre.}},
